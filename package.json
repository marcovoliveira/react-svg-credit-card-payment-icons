{
  "name": "react-svg-credit-card-payment-icons",
  "license": "MIT",
  "version": "5.1.0",
  "description": "React components library with 6 styles, 108 icons, TypeScript support, and a variety of types and formats for easy integration into React applications.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/marcovoliveira/svg-credit-card-payment-icons.git"
  },
  "homepage": "https://marcovoliveira.github.io/react-svg-credit-card-payment-icons",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./icons/*": {
      "types": "./dist/icons/*.d.ts",
      "import": "./dist/icons/*.mjs",
      "require": "./dist/icons/*.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "generate": "rm -rf generated/icons && tsx scripts/generate-components.ts",
    "dev": "tsx watch --clear-screen=false --exclude 'generated/**' --include 'src/icons/**/*.{yml,yaml,svg}' scripts/generate-components.ts",
    "pretypecheck": "pnpm run generate",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --config .prettierrc --write \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "format:check": "prettier --config .prettierrc --check \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "pretest": "pnpm run generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:storybook": "vitest --project=storybook",
    "prebuild": "pnpm run generate",
    "build": "tsup",
    "prestorybook": "pnpm run generate",
    "storybook": "storybook dev -p 6006",
    "prebuild-storybook": "pnpm run generate",
    "build-storybook": "storybook build -o docs-build"
  },
  "keywords": [
    "paypal",
    "svg",
    "paymenticons",
    "mastercard",
    "credit-card",
    "icons",
    "react",
    "credit",
    "components",
    "typescript",
    "logos",
    "brands",
    "icon",
    "react-component",
    "visa",
    "card",
    "payment"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.37.0",
    "@mdx-js/react": "^3.1.1",
    "@storybook/addon-docs": "^10.0.4",
    "@storybook/addon-links": "^10.0.4",
    "@storybook/addon-onboarding": "^10.0.4",
    "@storybook/addon-vitest": "^10.0.4",
    "@storybook/react-vite": "^10.0.4",
    "@svgr/cli": "^8.1.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jiti": "^2.6.1",
    "markdown-to-jsx": "^8.0.0",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sb": "^10.0.4",
    "storybook": "^10.0.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^3.2.4",
    "yaml": "^2.8.1",
    "eslint-plugin-storybook": "10.0.4"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  }
}
